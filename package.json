  },
  "homepage": "https://github.com/Denvy47/link_shortener#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "react-router-dom": "^5.2.0",
    "shortid": "^2.2.16"
  },
